<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Editor.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="Editor">

    <!-- <TextBlock Text="{Binding Greeting}" HorizontalAlignment="Center" VerticalAlignment="Center"/> -->
    <Grid>
        <Grid>
            <TextBox Name="SendDataTextBox" Height="30" Width="200" HorizontalAlignment="Right" VerticalAlignment="Top">
                <TextBox.Margin>130 35</TextBox.Margin>
                <TextBox.Text>Data to send</TextBox.Text>
            </TextBox>
            <Button Name="DataSend" Height="30" Width="60" HorizontalAlignment="Right" VerticalAlignment="Top" Click="DataSend_OnClick">
                <Button.Margin>50 35</Button.Margin>
                <Button.Content>Send</Button.Content>
            </Button>
        </Grid>
    </Grid>

</Window>
